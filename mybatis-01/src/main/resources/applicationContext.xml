<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context https://www.springframework.org/schema/context/spring-context.xsd">

    <bean id="testBean" class="spring.mvc.Bean.TestBean">
    </bean>
    <bean id="Homework" class="spring.mvc.Bean.Homework"></bean>
    <bean id="Student" class="spring.mvc.Bean.Student"></bean>
    <bean id="Student_Homework" class="spring.mvc.Bean.Student_Homework"></bean>
    <bean id="testAspect" class="spring.mvc.Aspect.testAspect"></bean>
    <bean id="StudentHomeworkJdbc" class="spring.mvc.Service.StudentHomeworkJdbc"></bean>
    <bean id="testJdbc" class="spring.mvc.Service.TestJDBC"></bean>


    <context:component-scan base-package="spring.mvc.Bean"/>
    <context:component-scan base-package="spring.mvc.Service"/>
    <context:component-scan base-package="spring.mvc.Aspect"/>
</beans>